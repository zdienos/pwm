<div class="continer">
    <div class="row">
        <div class="col-sm-2">

        </div>
        <div class="col-sm-8">
            <h3>Input data buku</h3>
            <form method="POST" action="/store_buku">
                <input type="hidden" name="_csrf" value="<%- _csrf %>" />
                <div class="form-group">
                    <label for="exampleInputJudulBuku">Judul Buku</label>
                    <input type="text" class="form-control" id="exampleInputJudulBuku" name="judul"
                        aria-describedby="emailHelp" placeholder="Judul Buku">
                </div>
                <div class="form-group">
                    <label for="exampleInputKeterangan">Keterangan</label>
                    <input type="text" class="form-control" id="exampleInputKeterangan" placeholder="Keteranagan"
                        name="keterangan">
                </div>

                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
        <div class="col-sm-2">

        </div>
    </div>
</div>